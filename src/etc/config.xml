<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <lowes_aus_post>
                <active>1</active>
                <name>Australia Post</name>
                <sallowspecific>0</sallowspecific>
                <model>ShipperHQ\Tracker\Model\Carrier\Tracker1</model>
                <title>Australia Post</title>
                <url><![CDATA[https://auspost.com.au/parcels-mail/track.html#/track?id=#TRACKNUM#]]></url>
            </lowes_aus_post>
            <lowes_aus_post_express>
                <active>1</active>
                <name>Australia Post Express</name>
                <sallowspecific>0</sallowspecific>
                <model>ShipperHQ\Tracker\Model\Carrier\Tracker2</model>
                <title>Australia Post Express</title>
                <url><![CDATA[https://auspost.com.au/parcels-mail/track.html#/track?id=#TRACKNUM#]]></url>
            </lowes_aus_post_express>
            <lowes_toll_priority>
                <active>1</active>
                <name>Toll Priority</name>
                <sallowspecific>0</sallowspecific>
                <model>ShipperHQ\Tracker\Model\Carrier\Tracker3</model>
                <title>Toll Priority</title>
                <url><![CDATA[https://online.toll.com.au/trackandtrace/traceConsignments.do?consignments=#TRACKNUM#]]></url>
            </lowes_toll_priority>
            <go_logistics>
                <active>1</active>
                <name>Go Logistics</name>
                <sallowspecific>0</sallowspecific>
                <model>ShipperHQ\Tracker\Model\Carrier\Tracker4</model>
                <title>Go Logistics</title>
                <url><![CDATA[https://www.gologistics.com.au/track-your-parcel?item=#TRACKNUM#]]></url>
            </go_logistics>
            <iws_dfe>
                <active>1</active>
                <name>Direct Freight Express</name>
                <sallowspecific>0</sallowspecific>
                <model>ShipperHQ\Tracker\Model\Carrier\Tracker5</model>
                <title>Direct Freight Express</title>
            </iws_dfe>
            <tg_express>
                <active>1</active>
                <name>TGExpress</name>
                <sallowspecific>0</sallowspecific>
                <model>ShipperHQ\Tracker\Model\Carrier\Tracker6</model>
                <title>TGExpress</title>
                <url><![CDATA[https://teamglobalexp.com/myparcel?shipmentID=#TRACKNUM#]]></url>
            </tg_express>
        </carriers>
        <extension_version>v1.0.6</extension_version>
    </default>
</config>
